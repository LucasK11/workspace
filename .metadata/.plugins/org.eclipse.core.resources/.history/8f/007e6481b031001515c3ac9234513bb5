import javax.swing.JOptionPane;


public class RollerCoaster {
public static void main(String[] args) {
String i =	JOptionPane.showInputDialog(null, "How tall are you?");

	
	int hi = Integer.parseInt(i);	
	if(hi<4){
	
	JOptionPane.showMessageDialog(null, "No it's time to Go on the roller coaster!!!!!!");
	
	
	
	}
		else if(hi>4){
	

			JOptionPane.showMessageDialog(null, "Sorry, your too small, come back when when you have grown more.");
	
	
	
		}
	
		else{
	
	
	
			JOptionPane.showMessageDialog(null, "Sorry, but thats not an awnser.");
	
	
	
	
		}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
}
